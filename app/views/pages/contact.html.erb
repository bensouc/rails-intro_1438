<h1>Contact</h1>
<form action="/contact" method="get">
  <input type="text" name="member" placeholder="Who are you looking for?" value=" <%= params[:query] unless params[:query].nil? %> ">
  <input type="submit">
</form>

<ul>
  <% @members.each do |member| %>
  <li> <%= member.capitalize %> </li>
  <% end %>
</ul>
<br>
<%= link_to "Retour Home", root_path %>
